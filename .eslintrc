{
   "settings": {
    "ecmascript": 5
  },
  "ecmaFeatures": {
  },
  "env": {
    "browser": true,
    "jquery": true
  },
  "rules": {
    semi: [2, "always"],
    no-undefined: 2,
    "indent": [2, 2],
    "quotes": [1, "single"],
    "key-spacing": [0, {"align": "value"}],
    "no-underscore-dangle": 0,
    "space-infix-ops": [2, {"int32Hint": false}],
    "no-trailing-spaces": 0,
    "no-mixed-spaces-and-tabs": 1
  }
}
